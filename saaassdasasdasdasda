if (leftIndex >= 0 || leftIndex <= rightIndex || rightIndex < array.length){
			boolean swapped = true;
			while(swapped){
				swapped = false;
				for(int i = leftIndex; i < rightIndex; i++){
					if (array[i].compareTo(array[i +1]) > 0){
						Util.swap(array, i, i + 1);
						swapped = true;
				}
			}
		}
	}
